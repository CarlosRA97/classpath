<defun link url text>
  <concat <A HREF="<get-var url>">
          <get-var text>
          </A>>
</defun>

<define-container package-list>
<UL>
%body
</UL>
</define-container>

<defun package-item name &optional section>
  <LI><link /software/classpath/status.html#<if <var-exists section> <get-var section> <get-var name>> <get-var name>></LI>
</defun>

<define-container package pname>
<H3><A NAME=<get-var pname>><get-var pname></A></H3>
%body
</define-container>

<define-container status name &optional component>
<BLOCKQUOTE>
<P>
  <b>Maintainer<if <match <get-var name> ","> "s"></b>: <get-var name><BR>
  <if <get-var component>
    <group <b>Component</b>: <get-var component>>>
</P>
<plain-text>
%body
</plain-text>
</BLOCKQUOTE>
<unset-var component>
<unset-var name>
</define-container>

